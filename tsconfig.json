{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "noResolve": false,
      "noImplicitAny": false,
      "removeComments": true,
      "sourceMap": true,
      "allowJs": true,
      "types": ["react"],

      "jsx": "react",
      
    "resolveJsonModule": true,
    "esModuleInterop": true,
    },
    "exclude": [
      "./node_modules/**/*",
    ],
  }